<IfModule mod_expires.c>
# Enable expirations
ExpiresActive On
# Default directive
ExpiresDefault "access plus 1 month"

# Images
ExpiresByType public/artnet_auctions.png "access plus 1 month"
ExpiresByType public/github-circle2.png "access plus 1 month"
ExpiresByType public/navicon7.jpg "access plus 1 month"
ExpiresByType public/pizza.png "access plus 1 month"
ExpiresByType public/portfolio.png "access plus 1 month"
ExpiresByType public/purple1.jpg "access plus 1 month"
ExpiresByType public/roman_forum.jpg "access plus 1 month"
ExpiresByType public/ruby_game1.jpg "access plus 1 month"
# CSS
ExpiresByType public/css "access plus 1 month"
# Javascript
ExpiresByType js "access plus 1 year"
</IfModule>
